<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>Result</title>
	<style>
		table {
			border: 1px solid black;
			border-collapse: collapse;
		}
	</style>
  </head>
  <body>
    <h1>Display GFAs Greater than or Equal to</h1>
    <table>
      <thead>
        <tr>
          <th>Name</th>
          <th>GPA</th>
        </tr>
      </thead>
      <tbody>
            <% applicants.forEach(applicant => { %>
                <tr>
                <td><%= applicant.name %></td>
                <td><%= applicant.gpa %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
    <a href="/">HOME</a>
  </body>
</html>
